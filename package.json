{
  "name": "green-heat",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:en": "ng serve --configuration=en",
    "build": "ng build && node scripts/copy-proxy-server.js",
    "build:sourcemap": "ng build --source-map",
    "build:localize": "ng build --localize && node scripts/copy-proxy-server.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "e2e": "ng e2e",
    "serve:ssr:green-heat": "node dist/green-heat/proxy-server.mjs",
    "extract:locales": "ng extract-i18n --output-path src/locale",
    "lint": "ng lint",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.1",
    "@angular/common": "^18.0.1",
    "@angular/compiler": "^18.0.1",
    "@angular/core": "^18.0.1",
    "@angular/forms": "^18.0.1",
    "@angular/platform-browser": "^18.0.1",
    "@angular/platform-browser-dynamic": "^18.0.1",
    "@angular/platform-server": "^18.0.1",
    "@angular/router": "^18.0.1",
    "@angular/ssr": "^18.0.2",
    "express": "^4.19.2",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.2",
    "@angular-eslint/builder": "18.0.1",
    "@angular-eslint/eslint-plugin": "18.0.1",
    "@angular-eslint/eslint-plugin-template": "18.0.1",
    "@angular-eslint/schematics": "18.0.1",
    "@angular-eslint/template-parser": "18.0.1",
    "@angular/cli": "^18.0.2",
    "@angular/compiler-cli": "^18.0.1",
    "@angular/localize": "^18.0.1",
    "@axe-core/puppeteer": "^4.9.1",
    "@compodoc/compodoc": "^1.1.25",
    "@puppeteer/ng-schematics": "^0.6.1",
    "@types/express": "^4.17.21",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^20.13.0",
    "@typescript-eslint/eslint-plugin": "7.17.0",
    "@typescript-eslint/parser": "7.17.0",
    "eslint": "8.57.0",
    "jasmine": "5.2.0",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-extract-i18n-merge": "^2.11.2",
    "puppeteer": "22.13.1",
    "source-map-explorer": "^2.5.3",
    "stylelint": "^16.6.1",
    "stylelint-config-sass-guidelines": "^11.1.0",
    "stylelint-scss": "^6.3.0",
    "typescript": "~5.5.4"
  }
}
